.page-preview {
  background-color: #fff;
  box-shadow: 0 0 5px 0 rgba(0, 0, 0, 0.2);
  box-sizing: border-box;
  margin: 50px auto;
  height: 795px;
  padding: 27px;
  width: 562px;
}

.page-preview__content {
  height: 100%;
  overflow: hidden;
}

.page-preview iframe {
  background-color: #fff;
  height: 100%;
  width: 100%;
}